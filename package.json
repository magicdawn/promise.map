{
  "name": "promise.map",
  "version": "1.1.2",
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "description": "Promise.map",
  "type": "module",
  "keywords": [
    "promise",
    "map",
    "parallel"
  ],
  "license": "MIT",
  "homepage": "https://github.com/magicdawn/promise.map#readme",
  "bugs": {
    "url": "https://github.com/magicdawn/promise.map/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magicdawn/promise.map.git"
  },
  "author": "magicdawn",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist/",
    "package.json"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "husky",
    "test": "vitest run",
    "test:dev": "vitest",
    "test-cover": "vitest run --coverage",
    "tsd": "tsd",
    "gen-readme": "swig render ./readme/readme.md > README.md && prettier --write README.md",
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@magicdawn/eslint-config": "^1.5.4",
    "@magicdawn/prettier-config": "^0.1.0",
    "@types/node": "^24.0.1",
    "@vitest/coverage-v8": "^3.2.3",
    "chance": "^1.1.13",
    "es-toolkit": "^1.39.3",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "should": "^13.2.3",
    "tsd": "^0.32.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{?(c|m)(j|t)s?(x),json,y?(a)ml}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!*.{?(c|m)(j|t)s?(x),json,y?(a)ml}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
